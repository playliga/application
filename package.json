{
  "name": "liga",
  "productName": "LIGA Esports Manager",
  "version": "2.0.0-beta.21",
  "description": "A FIFA clone for CS:GO",
  "main": "main.bundle.js",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lemonpole/LIGA",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/LIGA.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/LIGA/issues"
  },
  "keywords": [
    "manager",
    "e-sports",
    "esports",
    "simulator",
    "csgo"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r @babel/register ./webpack/compiler.js",
    "gen:free-agents": "ts-node -r tsconfig-paths/register ./scripts/free-agents.ts",
    "gen:generic-tiers": "ts-node -r tsconfig-paths/register ./scripts/generic-tiers.ts",
    "gen:pro-tiers": "ts-node -r tsconfig-paths/register ./scripts/pro-tiers.ts",
    "gen:new-db": "rm -rf resources/databases/save0.sqlite && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=development node -r @babel/register ./webpack/compiler.js",
    "start:electron": "cross-env NODE_ENV=development electron ./main.bundle.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm test -- --coverage"
  },
  "build": {
    "appId": "io.steverivera.liga",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app.category.games",
      "identity": "null"
    },
    "dmg": {
      "title": "${productName}"
    },
    "nsis": {
      "uninstallDisplayName": "${productName}"
    },
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "dist/renderer/screens/**/*",
      "main.bundle.js",
      "resources/**/*"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/register": "7.10.5",
    "@types/cheerio": "0.22.21",
    "@types/dedent": "0.7.0",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.158",
    "@types/minimist": "1.2.0",
    "@types/mock-fs": "3.6.30",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-transition-group": "4.4.0",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.46",
    "@types/sqlite3": "3.1.6",
    "@types/tail": "2.0.0",
    "@types/unzipper": "0.10.3",
    "@types/validator": "10.11.3",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "electron": "9.4.0",
    "electron-builder": "22.8.0",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.19.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.4.0",
    "mock-fs": "4.12.0",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.2",
    "sequelize-cli": "6.2.0",
    "style-loader": "1.2.1",
    "ts-jest": "25.4.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7",
    "url-loader": "2.2.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
