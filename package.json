    "dedent": "0.7.0",
    "duel": "3.2.0",
    "electron-is": "3.0.0",
    "electron-log": "4.2.2",
    "electron-updater": "4.3.4",
    "faker": "4.1.0",
    "flagpack": "1.0.5",
